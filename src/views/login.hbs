<h2 class="my-4 mx-5">Login</h2>
<form id="login-form" class="mx-5">
  <div class="form-group col-md-6">
    <label for="email">Email:</label>
    <input type="email" class="form-control" id="email" name="email" required>
  </div>
  <div class="form-group col-md-6">
    <label for="password">Password:</label>
    <input type="password" class="form-control" id="password" name="password" required>
  </div>
  <div class="form-group col-md-6 my-3">
    <button type="submit" class="btn btn-primary">Login</button>
    <a href="http://localhost:8080/mail" class="btn btn-danger">Restablecer contrase√±a</a>
    <a href="http://localhost:8080/session/github" class="btn btn-secondary"><i class="bi bi-github me-1"></i>Login with Github</a>
  </div>
</form>


<script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
<script type="module" src="js/login.js"></script>